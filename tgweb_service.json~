{
   "kind":"Service",
   "apiVersion":"v1beta3",
   "metadata":{
      "name":"tgweb",
      "labels":{
          "name":"tgweb"
      }
   },
   "spec":{
      "createExternalLoadBalancer": true,
      "ports": [
         {
           "port":3000,
           "targetPort":"http-server",
           "protocol":"TCP"
         }
      ],
      "selector":{
         "name":"tgweb"
      },
       "type": "LoadBalancer"
   }
}
